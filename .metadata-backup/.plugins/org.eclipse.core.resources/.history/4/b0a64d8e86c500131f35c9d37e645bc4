import java.util.Arrays;


public class backtracking.Test {
    public static void main(String[] args) {
    	
    	int[][] coord = 	 { {1,1},{1,4}, {2,2}, {5,3}};
    	int len = coord.length;
		int[] xcord = new int[len];
      	int[] ycord = new int[len];
      	 for(int row = 0; row < len; row++){
             xcord[row] = coord[row][0];
           }
          for(int row = 0; row < len; row++){
             ycord[row] = coord[row][1];
           }
        Arrays.sort(xcord);
        Arrays.sort(ycord);
    	System.out.println( (xcord[len-1]-xcord[0])*(ycord[len-1]-ycord[0]));
    }
    
   
   
}