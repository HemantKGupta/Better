
public class InorderSuccessorBST {

	public static void main(String[] args) {

	}
	
	public static BNode getInorderSuccessor(BNode root, int data){
		if(root == null)
			return null;
		
		if(data == root.data){
			BNode temp = root.right;
			if (temp == null){
				return root;
			}
			else if (temp != null){
				temp = temp.left;
			}
			
			return temp;
		}
		return null;
	}
	
}
