
public class EquiLeader {

	public static void main(String[] args) {

	}
	
	public static int solution(int[] A) {
        int result = 0;
        for (int i = 0; i < A.length; i++) {
        	
			
		}
        return result;
    }
	
	public static int leader(int[] A, int start, int end) {
		
		int len = end-start+1;
		if(start < end){
			return -1;
		}else if(len==1){
			return 0;
		}
		int size = 1;
		int val = A[start];
		int result = start;
		for (int i = 1; i < len; i++) {
			if (val == A[i]) {
				size++;
			} else {
				size--;
			}
			if (size == 0) {
				val = A[i];
				result = i;
				size++;
			}
		}
		int count = 0;
		for (int i = start; i < len; i++) {
			if (val == A[i]) {
				count++;
			}
		}
		if (count > len / 2) {
			return result;
		}
		return -1;  
    }

}
