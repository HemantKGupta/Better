import java.util.Arrays;

public class Interleavings {

	public static void main(String[] args) {
		String str1 = "AC";
		int m = str1.length();
		String str2 = "BD";
		int n = str2.length();
		char[] results = new char[m + n];
		printInterleavingsRecr(str1, str2, results, 0);

	}

	public static void printInterleavingsRecr(String str1, String str2,
			char[] iStr, int i) {
		int m = str1.length();
		int n = str2.length();

		if (m == 0 && n == 0) {
			System.out.println(Arrays.toString(iStr));
		}

		if (m != 0) {
			iStr[i] = str1.charAt(0);
			printInterleavingsRecr(str1.substring(1), str2, iStr, i + 1);
		}

		if (n != 0) {
			iStr[i] = str2.charAt(0);
			printInterleavingsRecr(str1, str2.substring(1), iStr, i + 1);
		}
	}

}
