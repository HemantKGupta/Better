
public class ReverseListIterative {

	public static void main(String[] args) {
		
	}
	
	public static Node reverse(Node head) {
		if (head == null)
			return head;

		Node current = head;
		Node previous = null;

		while (current != null) {
			Node nextNode = current.next;

			current.next = previous;
			previous = current;
			current = nextNode;
		}

		head = previous;
		return head;
	}

}
