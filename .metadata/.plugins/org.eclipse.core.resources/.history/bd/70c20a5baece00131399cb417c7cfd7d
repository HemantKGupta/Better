
public class DeleteAList {

	public static void main(String[] args) {
		
	}
	
	public static void deleteList(Node root){
		
	}
	
	/* Function to delete the entire linked list */
	public static void deleteList(Node root)
	{
	   /* deref head_ref to get the real head */
		Node  current = root;
		Node next;
	 
	   while (current != null) 
	   {
	       next = current.next;
	       current=null;
	       current = next;
	   }
	   
	   root = null;
	}

}
