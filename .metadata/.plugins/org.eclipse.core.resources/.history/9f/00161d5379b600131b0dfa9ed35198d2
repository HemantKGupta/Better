
public class GetNthFromLast {

	public static void main(String[] args) {

	}
	
	public static Node getNthNodeFromEnd(Node root, int n) {

		Node tmp = root;
		Node ntmp = root;
		for (int i = 1; i <= n; i++) {
			if (ntmp == null) {
				return null;
			}
			ntmp = ntmp.next;
		}
		while (ntmp != null) {
			ntmp = ntmp.next;
			tmp = tmp.next;
		}
		return tmp;

	}

}
