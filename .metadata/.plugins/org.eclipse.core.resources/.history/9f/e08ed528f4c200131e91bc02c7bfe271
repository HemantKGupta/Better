
public class MaxDoubleSliceSum2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int maxSliceForward(int[] A, int end){
		
		int max_ending = 0;
		int max_slice = 0;
		for (int i = 0; i <= end; i++) {
			int val = A[i];
			max_ending = Math.max(0, max_ending + val);
			max_slice = Math.max(max_slice, max_ending);
		}	
		return max_slice;

	}
	
public static int maxSliceBackward(int[] A, int start){
		
		int max_ending = 0;
		int max_slice = 0;
		for (int i = A.length; i >= start; i--) {
			int val = A[i];
			max_ending = Math.max(0, max_ending + val);
			max_slice = Math.max(max_slice, max_ending);
		}	
		return max_slice;

	}
	
	public static int maxSlice(int[] A){
		
		int max_ending = 0;
		int max_slice = 0;
		for (int val : A) {
			max_ending = Math.max(0, max_ending + val);
			max_slice = Math.max(max_slice, max_ending);
		}	
		return max_slice;

	}

}
