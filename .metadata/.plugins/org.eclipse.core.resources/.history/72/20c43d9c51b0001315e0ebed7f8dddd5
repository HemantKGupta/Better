
public class SimpleOperations {

	public static void main(String[] args) {

	}
	public static void printPostorder(Node node)
	{
	     if (node == null)
	        return;
	 	   
	     printPostorder(node.left);
	 	    
	     printPostorder(node.right);
	 	   
	     System.out.format("%d ", node.data);
	}
	 

}
