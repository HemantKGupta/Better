
public class MaxSliceExample {

	public static void main(String[] args) {
		int[] A ={5, -7, 3, 5, -2, 4, -1};
		System.out.println(maxSlice(A));

	}
	
	public static int maxSlice(int[] A){
		
		int max_ending = 0;
		int max_slice = 0;
		for (int val : A) {
			max_ending = Math.max(0, max_ending + val);
			max_slice = Math.max(max_slice, max_ending);
		}	
		return max_slice;

	}

}
