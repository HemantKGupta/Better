
public class FrogJmp {

	public static void main(String[] args) {
		System.out.println(solution(10, 85, 30));
	}
	
	public static int solution(int X, int Y, int D) {
		if(X==Y){
			return 1;
		}
		int noOfJumps = 0;
		for (int i=X; i<=Y;i=i+D ){
			noOfJumps++;
		}
		return noOfJumps;
	       
    }

}
