
public class SieveExample {

	public static void main(String[] args) {

	}
	
	public static boolean[] seive(int N){
		boolean[] seive = new boolean[N+1];
		seive[0]=seive[1]=false;
		int i=2;
		while (i*i<N){
			if (seive[i]== false){
				int k = i*i;
				while(k < N){
					seive[k]=false;
					k+=i;
				}
			}
			i++;
		}
		return seive;
	}

} 
