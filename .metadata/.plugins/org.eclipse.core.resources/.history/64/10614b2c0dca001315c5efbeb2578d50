	}
	
	// return position of rightmost set bit (1-32)
	public static int getRightMostSetBit(int n){
		int position  = 1;
		for (int i = 0; i< 32; i++){
			if ( (n & 1) == 1){
				return position;
			}else{
				n = n>>1;
				postion++
			}
		}
	}

}
