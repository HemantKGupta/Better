import java.util.Hashtable;


public class SubarraySumZero {

	public static void main(String[] args) {
		int[] ar = {4, 2, -3, 1, 6};
		
	
		
	}
	
	public static boolean testSubArray(int[] ar){
		Hashtable h = new Hashtable();
		for (int i = 0; i < ar.length; i++) {
			if (h.contains(-ar[i])){
				return true;
			}
				
		}
		return false;
	}

}
