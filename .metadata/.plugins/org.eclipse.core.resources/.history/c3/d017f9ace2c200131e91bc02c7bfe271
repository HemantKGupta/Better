
public class Dominator {

	public static void main(String[] args) {
		int [] A = {3,4,3,2,3,-1,3,3};
		System.out.println(solution(A));
	}
	
	public static int solution(int[] A) {
		int len = A.length;
		int size = 1;
		int val = A[0];
		for (int i = 1; i < len; i++) {
			if (val == A[i]) {
				size++;
			} else {
				size--;
			}
			if (size == 0) {
				val = A[i];
				size++;
			}
		}
		int count = 0;
		for (int i = 0; i < A.length; i++) {
			if (val == A[i]) {
				count++;
			}
		}
		if (count > len / 2) {
			return val;
		}
		return -1;  
    }
}
