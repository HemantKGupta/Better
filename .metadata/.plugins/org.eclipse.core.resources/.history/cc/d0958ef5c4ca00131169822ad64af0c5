import java.util.Arrays;


public class AllpermsWithRepitition {

	public static void main(String[] args) {
		printAllPermsWithRepitition("ABC");
	}
	
	public static void printAllPermsWithRepitition(String str){
		int len = str.length();
		char[] input = str.toCharArray();
		Arrays.sort(input);
		printPerms(input, 0, len-1);
		
	}
	
	public static void printPerms(char[] ar, int startIndex, int endindex){
		if (startIndex == endindex){
			System.out.println(Arrays.toString(ar));
		}
		for (int i = 0; i < ar.length; i++) {
			for (int j = i; j <= i; j++) {			
				char c = ar[i];
				char temp = ar[j];
				ar[j]=c;
				printPerms(ar,startIndex+1,endindex);
				ar[j]=temp;
			}
			
		}
	}

}
