import java.util.Arrays;

public class MaxProfit {

	public static void main(String[] args) {
		int[] A = new int[6];
		A[0] = 23171;
		A[1] = 21011;
		A[2] = 21123;
		A[3] = 21366;
		A[4] = 21013;
		A[5] = 21367;

	}

	public int solution(int[] A) {
		int len = A.length;
		int[] diff = new int[len];
		diff[0]=0;
		for (int i = 1; i < len; i++) {
			diff[i] = A[i+1]-A[i];
		}
		System.out.println(Arrays.toString(diff));
		return 0;
	}

}
