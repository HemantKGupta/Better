	public static int solution(int[] A) {
		int len = A.length;
		Arrays.sort(A);
		
		// case 1 all three positive		
		// case 2 one positive two negative
		int lowest = A[0];
		int nextlowest = 0;
	    if (lowest < 0){
			
			for (int i = 1; i < A.length; i++) {
				if( A[i] < 0 && A[i]!= lowest ){
					nextlowest = A[i];
					break;
				}
			}
	    }
		int largest = A[len-1];
		nextlowest = A[1];
		
