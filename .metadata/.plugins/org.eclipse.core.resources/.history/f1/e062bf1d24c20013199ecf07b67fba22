import java.util.Arrays;


public class GenomicRangeQuery3 {

	public static void main(String[] args) {
		String S = "GACACCATA";
		int[] P = {0, 0, 4, 7 };
		int[] Q = {8, 2, 5, 7 };
		System.out.println(Arrays.toString(solution(S, P, Q)));
	}
	
	public static int[] solution(String S, int[] P, int[] Q) {
		int n = S.length();
		int[][] count = new int[n][4];
		
		for (int i = 0; i < n; i++) {
			char val = S.charAt(i);
			switch (val) {	
				case 'A':
					count[i][0] = count[i][0]+ 1;
					break;
				case 'C':
					result = 2;
					break;
				case 'G':
					result = 3;
					break;
				case 'T':
					result = 4;
					break;
					
		    }
		
		
		return new int[0];
	}

}
