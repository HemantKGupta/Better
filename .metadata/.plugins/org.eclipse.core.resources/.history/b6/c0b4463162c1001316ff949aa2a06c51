public class PermMissingElem {

	public static void main(String[] args) {
		int[] A = { 2, 3, 1, 5 };
		System.out.println(solution(A));
	}

	public static int solution(int[] A) {
		int n = A.length;
		int requiredSum = (n + 1) * (n + 2) / 2;
		int sum = 0;
		for (int i = 0; i < A.length; i++) {
			sum += A[i];
		}

		return requiredSum - sum;

	}

}
