
public class PrimeNumbersExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static int divisors (int n){
		int i=1;
		int result = 0;
		while (i*i < n){
			if (n%i==0){
				result +=2;
			}
			i++;
		}
		if (i * i == n)
			result += 1;

		return result;
	}

}
