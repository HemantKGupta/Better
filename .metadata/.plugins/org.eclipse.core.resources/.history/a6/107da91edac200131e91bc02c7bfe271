
public class Fish {

	public static void main(String[] args) {

	}
	public static int solution1(int[] A, int[] B) {
		int len = B.length;
		boolean[] isAlive = new boolean[len];
		for (int i = 0; i < len; i++) {
			if(B[i]!= 1){
				isAlive[i]= true;
			}
			int currentSize = A[i];
			int currentIndex = i;
			for (int j = i+1; j < B.length; j++) {
				if (B[j]==0){
					if(currentSize < A[j]){
						isAlive[currentIndex]= false;
						currentIndex = j;
					}else if(currentSize > A[j]){
						isAlive[j]= false;						
					}
				} else if (B[j]==1) {
					currentSize = A[j];
					currentIndex = j;
				}
			}			
		}
		int countAlive = 0;
		for (int i = 0; i < len; i++) {
			if(isAlive[i]==true)
				countAlive++;
			
		}
		return countAlive;
	}

}
