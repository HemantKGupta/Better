
public class SingleSourceReachabilityDFSDigraph {

	public static void main(String[] args) {

	}
	
	 private static boolean[] marked; 
	
	 public static void dfs(Digraph G, int v) { 
	        marked[v] = true;
	        System.out.println("visited: "+ v);
	        for (int w : G.adj(v)) {
	            if (!marked[w]) dfs(G, w);
	        }
	    }

}
