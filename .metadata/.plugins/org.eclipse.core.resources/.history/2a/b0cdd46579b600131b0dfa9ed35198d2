
public class GetNthFromLast {

	public static void main(String[] args) {
		Node root = new Node(5);
		Node n1 = new Node(4);
		root.next = n1;
		Node n2 = new Node(3);
		n1.next = n2;
		Node n3 = new Node(2);
		n2.next = n3;
		Node n4 = new Node(1);
		n3.next = n4;
	}
	
	public static Node getNthNodeFromEnd(Node root, int n) {

		Node tmp = root;
		Node ntmp = root;
		// Get hold to nth node from start
		for (int i = 1; i <= n; i++) {
			if (ntmp == null) {
				return null;
			}
			ntmp = ntmp.next;
		}
		
		//
		while (ntmp != null) {
			ntmp = ntmp.next;
			tmp = tmp.next;
		}
		return tmp;

	}

}
