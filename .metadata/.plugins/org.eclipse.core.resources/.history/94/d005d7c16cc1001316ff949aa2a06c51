
public class PermCheck {

	public static void main(String[] args) {

	}
	
	 public static int solution(int[] A) {
		 int n = A.length;
		 int[] count = new int[n];
		 for (int i=0;i<A.length;i++){
			 if(A[i] > n ){
				 return 0;
			 }
			 if(count[A[i]] == 0){
				 count[A[i]]= 1;
			 }else if (count[A[i]] != 0){
				 return 1;
			 }
		 }
		 
		 
	    }

}
