public class ParityCount {

	public static void main(String[] args) {

	}

	public static int parity1(int x) {
		int result = 0;
		while (x > 0) {
			int val = (x & 1);
			result = (val == 1) ? result + 1 : result;
			x >>= 1;
		}
		return result;
	}

	public static int parity2(int x) {
		int result = 0;
		while (x > 0) {
			x = x & (x - 1);
			result++;
		}
		return result;
	}

}
