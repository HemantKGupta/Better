
public class AllStringPermutaions {

	public static void main(String[] args) {

	}
	
	public static void permute(char[] a, int i, int n) {
		   int j; 
		   if (i == n) // start equals end
		     printArray(a);
		   else
		   {
		        for (j = i+1; j <= n; j++){
		          swap(a, i, j);
		          permute(a, i+1, n);
		          swap(a, j, i); 
		       }
		   }
		}

}
