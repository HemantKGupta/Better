import java.util.Arrays;

public class AllStringPermutaions {

	public static void main(String[] args) {
		permute ("ABC".toCharArray(), 0, 2);
	}

	public static void permute(char[] a, int startIndex, int n) {
		   int j; 
		   if (startIndex == n) {// start equals end
		     System.out.println(Arrays.toString(a));
		   } else {
		        for (j = startIndex; j <= n; j++){
		          swap(a, startIndex, j);
		          permute(a, startIndex+1, n);
		          swap(a, j, startIndex); 
		       }
		   }
	}
	public static void swap(char[]a, int i, int j){
		char temp = a[i];
		a[i]= a[j];
		a[j] = temp;
	}
}
