
public class DFS {

	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static boolean[] marked; 
	// depth first search from v
	public static void dfs(Graph G, int v) {
		
			marked[v] = true;
			for (int w : G.adj(v)) {
				if (!marked[w]) {
					dfs(G, w);
				}
			}
		}

}
