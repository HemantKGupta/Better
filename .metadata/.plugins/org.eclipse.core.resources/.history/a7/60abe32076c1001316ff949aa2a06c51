import java.util.Arrays;


public class MaxCounters {

	public static void main(String[] args) {
		int[] A = {3,4,4,6,1,4,4};
		System.out.println(Arrays.toString(solution(5,A)));
	}
	
	public static int[] solution(int N, int[] A) {
		int m = A.length;
		int[] counters = new int[N];
		for (int i = 0; i < m; i++) {
			if (A[i] <= N){
				
			}else if (A[i] = N+1){
				
			}
			
		}
    }

}
