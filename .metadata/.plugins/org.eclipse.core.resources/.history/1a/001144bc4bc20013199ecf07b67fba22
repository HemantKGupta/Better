import java.util.Arrays;


public class Triangle {

	public static void main(String[] args) {

	}
	
	 public static int solution(int[] A) {
		 Arrays.sort(A);
		 int len = A.length;
		 int mid =  len/2;
		 int first = mid -1;
		 int last = mid+1;
		
		 int sum = first+mid+last;
		 if (sum > 2 * first && sum > 2* mid && sum > 2* last){
				  return 1;
			  }
		
		 
		 
	        return 1;
	        
	 }

}
