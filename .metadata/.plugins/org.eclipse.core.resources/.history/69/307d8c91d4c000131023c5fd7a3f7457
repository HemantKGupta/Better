	public static BNode build (BNode root, int[] in, int inStart, int inEnd, int[] pre, int preStart, int preEnd){
		int rootData = pre[preStart];
		root = new BNode(rootData);
		
		int rootIndexIn=-1;
		for (int i = 0; i< in.length;i++){
			if (in[i] == rootData){
				rootIndexIn=i;
				break;
			}
		}
		
		root.left = build(root.left,in, inStart, rootIndexIn-1, pre, preStart+1,preStart)
		root.right = 
		
		
		return null;
	}

}
