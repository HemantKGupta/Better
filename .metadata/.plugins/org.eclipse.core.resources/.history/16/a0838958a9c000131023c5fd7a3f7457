
public class TrieTest {

	public static void main(String[] args) {

	}
	
	public  static void insert(TrieNode root, String word) {
        int val = word.charAt(0) - 64;

       
        if (root.c[val] == null) {
        	root.c[val] = new TrieNode();
        }
        
        if (word.length() > 1) {
            insert(root,word.substring(1));
        } else {
            root.c[val].flag = true;
        }
    }
	
	public static boolean has(TrieNode root, String word) {
        int val = word.charAt(0) - 64;
        if (root.c[val]!=null && word.length()>1) {
            has(root, word.substring(1));
        } else if (root.c[val].flag==true && word.length()==1) {
            return true;
        }

        return false;
    }


}

class TrieNode{
	//make child nodes
    public TrieNode[] c;
    //flag for end of word
    public boolean flag = false;
}
