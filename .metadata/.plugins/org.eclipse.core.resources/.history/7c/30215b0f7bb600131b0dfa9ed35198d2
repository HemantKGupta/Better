
public class FindLoopAndBeginning {

	public static void main(String[] args) {

	}
	
	public static boolean testLoopInList(Node root) {

		Node slow = root;
		Node fast = root;

		while (slow != null && fast != null && fast.next != null) {
			slow = slow.next;
			fast = fast.next.next;
			if (slow == fast)
				return true;
		}
		return false;
	}

}
