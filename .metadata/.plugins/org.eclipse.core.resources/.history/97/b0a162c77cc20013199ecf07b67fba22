
public class Sigma2012 {

	public static void main(String[] args) {
		int[] H = {8,8,5,7,9,8,7,4,8};
	}
	
	 public static int solution(int[] H) {
		 int len = H.length;
		 int min = H[0];
		 int result = 0;
		 for (int i = 1; i < H.length; i++) {
			 int val = H[i];
			 if (val < min){
				 min = val;
			 }
		 }
		 if (min != 0){
			 for (int i = 0; i < H.length; i++) {
					H[i]= H[i]-min;
			  }
			 result++;
		 }
		 int zeroIndex=0;
		 for (int i = 0; i < H.length; i++) {
			 int val = H[i];
			 if (val == 0){
				 zeroIndex = i; 
			 }
		  }
		
		 
		 
		 
	  
		 return 0; 
	 }

}
