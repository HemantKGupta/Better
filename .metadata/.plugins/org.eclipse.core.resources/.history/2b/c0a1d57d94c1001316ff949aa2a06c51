import java.util.Arrays;

public class GenomicRangeQuery {

	public static void main(String[] args) {
		String S = "GA";//CACCATA";
		int[] P = {0};//, 0, 4, 7 };
		int[] Q = {1};//, 2, 5, 7 };
		System.out.println(Arrays.toString(solution(S, P, Q)));
	}

	public static int[] solution(String S, int[] P, int[] Q) {
		int M = P.length;
		int[] result = new int[M];
		for (int i = 0; i < M; i++) {
			result[i] = findMinInString(S, P[i], Q[i]);
		}
		return result;

	}

	private static int findMinInString(String s, int i, int j) {
		int result = 0;
		char min;
		if (i == j) {
			 min = s.charAt(i);
		} else {
			char[] dst = new char[j - i];
			s.getChars(i, j, dst, 0);
			min = dst[0];
			for (char c : dst) {
				if (min > c) {
					min = c;
				}

			}
		}
		System.out.println(min);
		switch (min) {
		case 'A':
			result = 1;
			break;
		case 'C':
			result = 2;
			break;
		case 'G':
			result = 3;
			break;
		case 'T':
			result = 4;
			break;
		}
		//System.out.println(result);
		return result;
	}

}
