
public class Fish {

	public static void main(String[] args) {
		int[] A = {4,3,2,1,5};
		int[] B = {0,1,0,0,0};
		System.out.println(solution(A,B));
		
	}
	public static int solution(int[] A, int[] B) {
		int len = B.length;
		if(len == 1){
			return 1;
		}else if (len==2){
			if (B[0]==1 && B[1]==0){
				return 1;
			}else if (B[0]==B[1]){
				return 2;
			}
		}
		boolean[] isAlive = new boolean[len];
		int i = 0;
		for (i = 0; i < len; i++) {
			if(B[i]!= 1){
				isAlive[i]= true;
			} else {
				isAlive[i]= true;
				break;
		}
		System.out.println("first index is i:"+ i);
		int currentSize = A[i];
		int currentIndex = i;
			for (int j = i+1; j < B.length; j++) {
				if (B[j]==0){
					if(currentSize < A[j]){
						isAlive[currentIndex]= false;
						currentIndex = j;
					}else if(currentSize > A[j]){
						isAlive[j]= false;						
					}
				} else if (B[j]==1) {
					currentSize = A[j];
					currentIndex = j;
				}
			}			
		
		int countAlive = 0;
		for (int k = 0; k < len; k++) {
			if(isAlive[k]==true)
				countAlive++;
			
		}
		return countAlive;
	

}
