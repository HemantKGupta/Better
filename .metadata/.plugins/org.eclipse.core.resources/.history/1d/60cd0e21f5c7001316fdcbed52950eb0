
public class ParityTest {

	public static void main(String[] args) {
		System.out.println(getParity(3));

	}
	public static boolean getParity(int n)
	{
	    boolean parity = false;
	    while (n>0)
	    {
	        parity = !parity;
	        n      = n & (n - 1);
	    }        
	    return parity;
	}

}
