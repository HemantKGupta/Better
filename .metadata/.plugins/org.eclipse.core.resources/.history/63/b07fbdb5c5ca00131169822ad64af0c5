import java.util.Arrays;


public class AllpermsWithRepitition {

	public static void main(String[] args) {
		printAllPermsWithRepitition("ABC");
	}
	
	public static void printAllPermsWithRepitition(String str){
		int len = str.length();
		char[] input = str.toCharArray();
		char[] print = new char[len];
		Arrays.sort(input);
		printPerms(input, print, len -1, 0);
		
	}
	
	public static void printPerms(char[] ar, char[] print, int last, int index){
		
		for (int i = 0; i < ar.length; i++) {
			
			print[index] = ar[i];
			
			if (last == index){
				System.out.println(Arrays.toString(print));
			}else{
				printPerms(ar, print, last, index+1);
			}
			
		}
	}

}
