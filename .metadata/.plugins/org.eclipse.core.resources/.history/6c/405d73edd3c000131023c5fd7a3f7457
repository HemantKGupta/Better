
public class BuildTreeInPre {

	public static void main(String[] args) {

	}
	
	public static BNode buildTree(int[] in, int[] pre){
		
		return null;
	}
	
	public static BNode build (int[] in, int inStart, int inEnd, int[] pre, int preStart, int preEnd){
		int rootData = pre[preStart];
		
		int rootIndexIn=-1;
		for (int i = 0; i< in.length;i++){
			if (in[i] == rootData){
				rootIndexIn=i;
				break;
			}
		}
		
		
		
		
		return null;
	}

}
