
public class SubTreeTest {

	public static void main(String[] args) {

	}
	
	public static boolean identicalTrees(BNode a, BNode b) {
		if (a == null && b == null)
			return true;

		if (a != null && b != null) {
			return (a.data == b.data &&
					identicalTrees(a.left, b.left) &&
					identicalTrees(a.right, b.right));
		}
		return false;
	}


}
